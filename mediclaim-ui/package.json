  "license": "APACHE-2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rm -rf ./dist && tsc && vite build --mode testnet && cp -r ../contract/src/managed/dawn/keys ./dist/keys && mkdir -p ./dist/zkir && cp ../contract/src/managed/dawn/zkir/*.zkir ./dist/zkir/",
