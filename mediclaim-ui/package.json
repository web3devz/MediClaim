    "ci": "npm run typecheck && npm run lint",
    "dev": "vite --host 0.0.0.0",
    "lint": "eslint ./src",
    "start": "http-server --port 0 ./dist",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "preview": "vite preview"
  },
